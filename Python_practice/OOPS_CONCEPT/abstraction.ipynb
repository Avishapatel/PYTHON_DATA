    "from abc import ABC,abstractmethod\n",
    "class Animal(ABC):\n",
    "    @abstractmethod\n",
    "    def speak(self):\n",
    "        print(\"hello\")\n",
    "class dog(Animal):\n",
    "    def speak(self):\n",
    "        print(\"bhow bhow\")\n",
    "    def data(self):\n",
    "        super().speak()\n",
    "        print(\"data\")\n",
    "d=dog()\n",
    "d.speak()\n",
    "d.data()"
   ]
  }
